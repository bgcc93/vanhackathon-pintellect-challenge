{
	"name": "Pintellect",
	"version": "0.0.2",
	"manifest_version": 2,
	"description": "Pintellect scrapper",
	"icons": {
		"16": "images/pintellect-logo.png",
		"128": "images/pintellect-logo.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": [
			"scripts/background.js"
		]
	},
	"permissions": [
		"tabs",
		"http://localhost:3000/"
	],
	"browser_action": {
		"default_icon": {
			"19": "images/pintellect-logo.png",
			"38": "images/pintellect-logo.png"
		},
		"default_title": "Pintellect",
		"default_popup": "popup.html"
	}
}
